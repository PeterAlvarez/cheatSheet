package grafoPonderado;

public class Principal {
	public static void main(String[] args) {
		GrafoPonderado g = new GrafoPonderado(10);
		
		g.insertarNodo("A");
		g.insertarNodo("B");
		g.insertarNodo("C");
		g.insertarNodo("D");
		g.insertarNodo("E");
		g.insertarNodo("F");
		
		g.insertarArco("A", "B", 3);
		
		System.out.println(g.toString());
		
		
	}
}

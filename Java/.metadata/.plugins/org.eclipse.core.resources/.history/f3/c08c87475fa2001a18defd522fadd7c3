package NumerosStirling;

public class NumStirlingNoOrdenados {
	public static void main(String[] args) {
		int filasTriangulo = 20;
		long [] [] TP = new long [filasTriangulo+1] [filasTriangulo+1] ;
		
		for(int j = 0; j < TP.length; j++) {
			TP[j] [0] = 1;
			TP[j] [j] = 1;
		}
		
		for(int i = 1; i < TP.length; i++) {
			for(int j = 1; j < i; j++) {
				TP [i] [j] = TP [i-1] [j-1] + ( TP[i-1] [j] * ( j-1 ) );
			}
		}
		
		imprimirTP(TP);
		
		int m = 20;
		int n = 10;
		System.out.println("");
		System.out.println(" Cm,n="+"C"+m+","+n+"="+TP[m][n]);
		
	}

	private static void imprimirTP(long[][] tp) {
	     for(int i = 0; i < tp.length; i++) {
	    	 System.out.println("");
	    	 for(int j = 0; j <= i; j++) {
	    		 System.out.print(" "+tp[i][j]);
	    	 }
	    	 System.out.println("");
	     }
	}
}

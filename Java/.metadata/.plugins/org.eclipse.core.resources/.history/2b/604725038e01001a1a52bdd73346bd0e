package grafoPonderado;

public class Principal {
	public static void main(String[] args) {
		GrafoPonderado g = new GrafoPonderado(10);
		
		g.insertarNodo("A");
		g.insertarNodo("B");
		g.insertarNodo("C");
		g.insertarNodo("D");
		g.insertarNodo("E");
		g.insertarNodo("F");
		
		g.insertarArco("A", "B", 3);
		g.insertarArco("C", "D", 5);
		g.insertarArco("F", "E", 6);
		g.insertarArco("A", "C", 7);
		g.insertarArco("B", "E", 4);
		g.insertarArco("D", "F", 1);
		
		System.out.println(g.toString());
		
		
	}
}
